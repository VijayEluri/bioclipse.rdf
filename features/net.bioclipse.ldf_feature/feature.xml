<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="net.bioclipse.ldf_feature"
      label="Linked Data Fragments Feature"
      version="2.6.2.qualifier"
      provider-name="Bioclipse Team"
      plugin="net.bioclipse.ldf">

   <description>
      Linked Data Fragments support for Bioclipse.
